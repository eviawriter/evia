    "electron-reload": "^1.2.2",
    "install": "^0.11.0",
    "npm": "^5.8.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "node.js": "0.0.0",
    "quill": "^1.3.6",
    "sqlite3": "^4.0.0"
